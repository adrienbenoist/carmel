{
  "name": "challenges",
  "routes": {
    "main": {
      "title": "Challenges",
      "icon": "favorite",
      "title": "Take a challenge",
      "action": "true",
      "path": "/challenges",
      "startOperationsOnMount": false,
      "variants": "initial",
      "operations": {
        "newActivity": [
          "rest://create/journey/activities#newActivity",
          {
            "onSuccess": "activityAdded",
            "onError": "activityError"
          }
        ]
      }
    }
  }
}
